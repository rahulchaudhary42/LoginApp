<?xml version="1.0" encoding="UTF-8"?>
<web-app>

<servlet>
<servlet-name>RegisterServlet</servlet-name>
<servlet-class>com.bridgelabz.controller.RegisterServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>RegisterServlet</servlet-name>
